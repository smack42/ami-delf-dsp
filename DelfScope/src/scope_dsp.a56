	include 'delfequ.inc'

	org	p:

;->prog+0
	jmp	convert



;->prog+2

;***************************************************************************
;*** DelfModule - receives the input data
	move	y:(r0),r0		;input pipe
	move	y:(r1),r1		;output pipe
	move	y:y_bufpt,r4
	movec	#2048-1,m4		;m4 - modulo addressing
	do	#128,mod_receive_loop
	move	l:(r0)+,x
	move	x,l:(r4)+
	move	x,l:(r1)+
mod_receive_loop
	move	r4,y:y_bufpt

	move	y:y_mod_count,a
	move	#>1,x0
	add	x0,a	a,x1
	teq	x1,a			;do not count while (mod_count==-1)
	move	a,y:y_mod_count
	rts


;***************************************************************************
;*** convert PCM samples
;
;  -calculate average amplitudes (into x:avg_l, x:avg_r)
;  -plot_type==1: 24bit->8bit, half sampling rate (into l:buf8)
;  -plot_type==2: spectrum analyzer
;
; x1 ... number_of_samples
; x0 ... plot_type  (1 or 2)
; y1 ... /
; y0 ... /
convert
	move	x1,n0			;numsamp			!! n0
	move	x0,y:y_plot_type
	move	y:y_bufpt,r0
	movec	#2048-1,m0		;m0 - modulo-addressing
	move	r0,x:x_busy		;set BUSY
	move	(r0)-n0

;******	calculate average amplitude
	move	r0,r6
	movec	m0,m6			;m6 - modulo-addressing
	move	#>$002000,y1		;shift right by 10 bits
	clr	b	#<0,x0
	move		x:(r6),a	b,y0
	do	n0,avg_amp_loop
	abs	a	y:(r6)+,b
	abs	b	a,x1
	mpy	x1,y1,a	b,x1
	mpy	x1,y1,b
	add	x0,a
	add	y0,b	a,x0
	move		x:(r6),a	b,y0
avg_amp_loop
	move	y:y_plot_type,n1	;jmp
	move	#>y_plot_jmp,r1		;jmp
	move	x0,x:x_avg_l		;must be divided by numsamp for avg
	move	y0,x:x_avg_r		;must be divided by numsamp for avg
	move	y:(r1+n1),r1		;jmp
	movec	m7,m6			;m6 - linear addressing
	jmp	(r1)


;******	plot one: convert 24bit samples to 8bit (into l:buf8)
plot_1	move	n0,a			;numsamp
	move	#>5,x0
	add	x0,a	#>6,x0
	move	a,x1
	jsr	<delf_div		;6 samples per loop
	rnd	a	#>l_buf8,r1
	ori	#$04,mr			;SCALE DOWN mode
	do	a1,p248_loop
	move		l:(r0)+,ab
	move		x:(r0),x1
	add	x1,a	y:(r0)+,x1
	add	x1,b	#>$ff0000,y1
	asr	a
	asr	b
	and	y1,a
	and	y1,b	a1,x0
	move		b1,y0
	move		l:(r0)+,ab
	move		x:(r0),x1
	add	x1,a	y:(r0)+,x1
	add	x1,b	a,x1	;scale down
	mpy	x1,#8,a
	move		#>$00ff00,y1
	and	y1,a	b,x1	;scale down
	mpy	x1,#8,b
	or	x0,a
	and	y1,b	a1,x0
	or	y0,b
	move		b1,y0
	move		l:(r0)+,ab
	move		x:(r0),x1
	add	x1,a	y:(r0)+,x1
	add	x1,b	a,x1	;scale down
	mpy	x1,#16,a
	move		#>$0000ff,y1
	and	y1,a	b,x1	;scale down
	mpy	x1,#16,b
	or	x0,a
	and	y1,b	a1,x:(r1)
	or	y0,b
	move		b1,y:(r1)+
p248_loop
	andi	#$f3,mr			;NO SCALING mode
	movec   m7,m0			;m0 - linear addressing
	move	#<0,x0
	move	x0,x:x_busy		;clear BUSY
	rts



;******	plot two: spectrum analyzer
plot_2	move	#>l_fft_in,r7
	move	y:y_bufpt,r0
	move	#1024,n0
	move	#$002000,x1		;#$002000 - shift right by 10 bits
	move	(r0)-n0
	move	r7,r1			;#>l_fft_in
	move	#>y_fft_window,r2
	clr	b		x:(r0)+,x0
	do	#1024,fft_input_left
	mpyr	x0,x1,a
	move	a,x0		y:(r2)+,y0
	mpyr	x0,y0,a		x:(r0)+,x0
	move			ab,l:(r1)+	;set re, clear im
fft_input_left

;FFTR2CN
;
; This program originally available on the Motorola DSP bulletin board.
; It is provided under a DISCLAMER OF WARRANTY available from
; Motorola DSP Operation, 6501 Wm. Cannon Drive W., Austin, Tx., 78735.
; 
; Radix 2, In-Place, Decimation-In-Time FFT (fast).
; 
	move	#>l_fft_sincos,n7

	move	r7,r0			;#>l_fft_in
	move	#1024/4,n0
	move	n0,n4
	move	n0,n6
	movec	#1024-1,m0		;m0 - modulo addressing
	movec	m0,m1			;m1 - modulo addressing
	movec	m0,m4			;m4 - modulo addressing
	movec	m0,m5			;m5 - modulo addressing
	move			x:(r0)+n0,x0
	tfr	x0,a		x:(r0)+n0,y1   
	do	n0,fft_twopass
	tfr	y1,b		x:(r0)+n0,y0
	add	y0,a		x:(r0),x1
	add	x1,b		r0,r4
	add	a,b		(r0)+n0
	subl	b,a		b,x:(r0)+n0
	tfr	x0,a		a,x0		y:(r0),b
	sub	y0,a				y:(r4)+n4,y0
	sub	y0,b		x0,x:(r0)
	add	a,b				y:(r0)+n0,x0
	subl	b,a		b,x:(r0)
	tfr	x0,a		a,x0		y:(r4),b
	add	y0,a				y:(r0)+n0,y0
	add	y0,b		x0,x:(r0)+n0
	add	b,a				y:(r0)+,x0
	subl	a,b				a,y:(r4)+n4
	tfr	x0,a				b,y:(r4)+n4
	sub	y0,a		x1,b
	sub	y1,b		x:(r0)+n0,x0
	add	a,b		x:(r0)+n0,y1
	subl	b,a				b,y:(r4)+n4
	tfr	x0,a				a,y:(r4)+
fft_twopass

	move	#1024/8,n1		;butterflies per group
	move	#<4,n2			;groups per pass
	movec	#<0,m6			;m6 - bit-reversed addressing
	do	#10-3,fft_pass
	move	r7,r0			;#>l_fft_in
	move	r0,r1
	move	n1,r2
	move	r0,r4
	move	(r1)+n1
	move	r1,r5
	move	n7,r6			;#>l_fft_sincos
	lua	(r2)+,n0
	move	n0,n4
	move	n0,n5
	move	(r2)-			;butterfly loop count
	move			x:(r1),x1	y:(r6),y0
	move			x:(r6)+n6,x0	y:(r0),b
	mac	x1,y0,b				y:(r1)+,y1
	macr	-x0,y1,b			y:(r0),a
	do	n2,fft_grp
	do	r2,fft_bfy
	subl	b,a		x:(r0),b	b,y:(r4)
	mac	-x1,x0,b	x:(r0)+,a	a,y:(r5)
	macr	-y1,y0,b	x:(r1),x1
	subl	b,a		b,x:(r4)+	y:(r0),b
	mac	x1,y0,b				y:(r1)+,y1
	macr	-x0,y1,b	a,x:(r5)+	y:(r0),a
fft_bfy	move	(r1)+n1
	subl	b,a		x:(r0),b	b,y:(r4)
	mac	-x1,x0,b	x:(r0)+n0,a	a,y:(r5)
	macr	-y1,y0,b	x:(r1),x1	y:(r6),y0
	subl	b,a		b,x:(r4)+n4	y:(r0),b
	mac	x1,y0,b		x:(r6)+n6,x0	y:(r1)+,y1
	macr	-x0,y1,b	a,x:(r5)+n5	y:(r0),a
fft_grp	move	n1,b
	lsr	b	n2,a	;divide butterflies per group by two
	lsl	a	b,n1	;multiply groups per pass by two
	move	a,n2
fft_pass

	move	#<2,n0
	move	n0,n1
	move	#1024/4,n4
	move	n4,n5
	move	r7,r0			;#>l_fft_in
	move	#>l_fft_out,r4
	move	r4,r2
	lua	(r0)+,r1
	lua	(r2)+n2,r5
	movec	#<0,m4			;m4 - bit-reversed addressing
	movec	m4,m5			;m5 - bit-reversed addressing
	move	n7,r6			;#>l_fft_sincos
	move	(r5)-n5
	move			x:(r1),x1	y:(r6),y0
	move			x:(r5),a	y:(r0),b

	do	n2,fft_lastpass
	mac	x1,y0,b		x:(r6)+n6,x0	y:(r1)+n1,y1
	macr	-x0,y1,b	a,x:(r5)+n5	y:(r0),a
	subl	b,a		x:(r0),b	b,y:(r4)
	mac	-x1,x0,b	x:(r0)+n0,a	a,y:(r5)
	macr	-y1,y0,b	x:(r1),x1	y:(r6),y0
	subl	b,a		b,x:(r4)+n4	y:(r0),b
fft_lastpass
	move			a,x:(r5)+n5

	movec   m7,m0			;m0 - linear addressing
	movec   m7,m1			;m1 - linear addressing
	movec   m7,m4			;m4 - linear addressing
	movec   m7,m5			;m5 - linear addressing
	movec   m7,m6			;m6 - linear addressing
;------------------------------------------------------------------------




;convert complex output values to magnitude (dB-scale)
	move	#>l_fft_out,r0
	move	#>l_buf8,r1

	do	#416,fft_magnitude
	move			x:(r0),x0
	mpy	x0,x0,a		y:(r0)+,y0
	mac	y0,y0,a		#<$40,x0	;a=yr**2+yi**2
	move	a1,y1				;transfer sum of squares to y
	move	a0,y0

	clr	b		x0,x1	;init root
	do	#23,fft_mag_sqrt
	mpy	-x0,x0,a		;square and negate the guess
	add	y,a			;compare to double precision input
	tge	x0,b			;update root if input >= guess
	tfr	x1,a			;get bit to test
	asr	a			;shift to next bit to test
	add	b,a		a,x1	;form new guess
	move	a,x0			;save new guess
fft_mag_sqrt

	move	#>y_fft_log_coef,r2
	move	#>$000008,a
	move			y:(r2)+,y1	;get dB-offset
	cmp	a,b		#-0.6228,a	;log of zero:  -120 dB
	jle	fft_logzero
	movec	m7,r7			;#$ffff
	rep	#23			;normalize (0.5 ... 1.0)
	norm	r7,b			;shift left, decrement r7 if needed
	move	b,x0
	mpyr	x0,x0,a		y:(r2)+,y0	;x**2, get c1
	mpy	x0,y0,a	a,x1	y:(r2)+,y0	;c1*x, mv x**2, get c2
	mac	x1,y0,a		y:(r2)+,y0	;c2* x**2, get c0
	add	y0,a				;add in c0
	asl	a				;multiply by 4
	asl	a
	asl	a				;shift out sign bit
	move	r7,a2			;new sign = characteristic
	rep	#6
	asr	a			;divide by 32, create sign bit
fft_logzero
	add	y1,a	#<0,y0		;+ dB-offset
	tmi	y0,a
	move	#0.8027486,y0		;1/0.31143*0.25  (display 60 dB range)
	move	a,x0
	mpy	x0,y0,a
	asl	a
	asl	a			;*4
	move	a,x:(r1)+		;save result

fft_magnitude

	move	#<0,x0
	move	x0,x:x_busy		;clear BUSY
	rts








;***************************************************************************

	org	x:
;->xdata+0
x_busy		ds	1
;->xdata+1
x_avg_l		ds	1
;->xdata+2
x_avg_r		ds	1




